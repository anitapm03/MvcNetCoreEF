@model List<HospitalEF>

<h1>Hospitales</h1>

<table class="table table-dark">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Direccion</th>
            <th>Telefono</th>
            <th>Camas</th>
        </tr>
    </thead>
    <tbody>
        @foreach (HospitalEF h in Model)
        {
            <tr>
                <td>@h.Nombre</td>
                <td>@h.Direccion</td>
                <td>@h.Telefono</td>
                <td>@h.Camas</td>
            </tr>
        }
    </tbody>
</table>
